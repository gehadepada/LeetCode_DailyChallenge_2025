
class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>>v;
        v.push_back({1});
        if(numRows>=2)
        {
        v.push_back({1,1});
        }
        numRows-=2;
        vector<int>last;
        while(numRows>0)
        {
            vector<int>a;
            a.push_back(1);
            last=v.back();
        
            for(int i =1 ; i <last.size() ; i ++)
            {
                a.push_back(last[i]+last[i-1]);
            }
            numRows--;
            cout<<endl;
            a.push_back(1);
            v.push_back(a);
          
        }
        return v;


        
    }
};
